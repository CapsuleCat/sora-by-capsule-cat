{
  "name": "sora-by-capsule-cat",
  "displayName": "Sora by Capsule Cat",
  "description": "",
  "version": "0.0.2",
  "license": "MIT",
  "publisher": "CapsuleCat",
  "repository": {
    "type": "git",
    "url": "git@github.com:CapsuleCat/sora-by-capsule-cat.git"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sora-by-capsule-cat.setApiKey",
        "title": "Sora: Set API Key"
      }
    ],
    "configuration": [
      {
        "title": "Sora by Capsule Cat",
        "properties": {
          "sora-by-capsule-cat.apiKey": {
            "type": "string",
            "default": "",
            "description": "Your OpenAI API key."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.13",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "openai": "^3.2.1"
  }
}
